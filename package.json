{
  "name": "vibefunder",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "lsof -ti:3900 | xargs -r kill -9 2>/dev/null || true && next dev --port 3900",
    "build": "lsof -ti:3900 | xargs -r kill -9 2>/dev/null || true && next build",
    "postinstall": "prisma generate",
    "test": "npx tsx scripts/universal-test-runner.ts",
    "test:unit": "npx tsx scripts/universal-test-runner.ts src/__tests__/unit/",
    "test:integration": "npx tsx scripts/universal-test-runner.ts src/__tests__/integration/",
    "test:ai": "npx tsx scripts/universal-test-runner.ts src/__tests__/ai/",
    "test:api": "npx tsx scripts/universal-test-runner.ts src/__tests__/api/",
    "test:security": "npx tsx scripts/universal-test-runner.ts src/__tests__/security/",
    "test:watch": "npx tsx scripts/universal-test-runner.ts --watch",
    "test:coverage": "npx tsx scripts/universal-test-runner.ts --coverage",
    "start": "next start",
    "lint": "next lint",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "@prisma/client": "^6.13.0",
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/server": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.0.3",
    "nodemailer": "^6.9.8",
    "openai": "^5.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stripe": "^16.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.5.0",
    "eslint-config-next": "^15.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.4.47",
    "prisma": "^6.13.0",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
